{
  "name": "app-ng-ssr-icarion",
  "version": "3.6.0-rc.0",
  "scripts": {
    "ng": "ng",
    "dev:ssr": "ng run app-ng-ssr-icarion:serve-ssr",
    "dev:ssr:icarion-es": "(cp .env-icarion-es .env || copy .env-icarion-es .env) && npm run dev:ssr",
    "dev:ssr:icarion-pt": "(cp .env-icarion-pt .env || copy .env-icarion-pt .env) && npm run dev:ssr",
    "dev-mockserver": "npm run start-proxy & npm run mockserver",
    "dev-mockserver-ssr": "npm run dev:ssr & npm run mockserver",
    "serve:ssr": "node dist/app-ng-ssr-icarion/server/main.js",
    "build:ssr": "ng build --configuration production && ng run app-ng-ssr-icarion:server:production",
    "prerender": "ng run app-ng-ssr-icarion:prerender",
    "start": "ng serve",
    "start-proxy-es": "ng serve -o --proxy-config proxy.conf.es.json",
    "start-proxy-pt": "ng serve -o --proxy-config proxy.conf.pt.json",
    "build": "ng build",
    "test": "ng test --watch=false --browsers=Chrome --code-coverage",
    "lint": "npm-run-all -p lint:*",
    "lint:app": "eslint -c .eslintrc.json --ext .ts src/app --fix",
    "lint:server": "eslint -c .eslintrc.json --ext .ts server --fix",
    "report": "ng build --configuration production --stats-json && webpack-bundle-analyzer dist/app-ng-ssr-icarion/browser/stats.json",
    "mockserver": "mockserver -p 8080 -m test/mocks",
    "prebuild:ssr": "ngcc"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.11",
